        var GALLERY_PATH = 'http://echarts.baidu.com/';
        console.log(GALLERY_PATH);
        var GALLERY_EDITOR_PATH = 'editor?c=';
        var GALLERY_VIEW_PATH = GALLERY_PATH + 'view.html?c=';
        var GALLERY_THUMB_PATH = GALLERY_PATH + 'gallery/data/thumb/';
    </script>

    <script type="text/javascript">
     document.getElementById('nav-examples').className = 'active';
        function loadDemo() {
            var chartId = location.hash.slice(1);
            $('#chart-demo').html('<iframe src="' + GALLERY_EDITOR_PATH + chartId + '"></iframe>');
        }

        $(window).on('hashchange', function () {
            loadDemo();
        });
        loadDemo();
    </script>
}

